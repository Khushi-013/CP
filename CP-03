//FUNCTION to find sum of two integers (Function with parameter passing)

# include <stdio.h>
int add(int a,int b)
{
    int c=a+b;
}
int main()
{
    int sum;
    sum=add(5,4);
    printf("Sum is %d\n",sum);
    //As we are not using void hence we have to use return value
    return 0;
}
